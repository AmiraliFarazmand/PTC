<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_0sbemye" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.34.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.6.0">
  <bpmn:process id="LoginProcess" name="login" isExecutable="true">
    <bpmn:startEvent id="login_request_event" name="user sends login request">
      <bpmn:outgoing>Flow_0e4mtwd</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="create-token-task" name="create token">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="create-login-token" />
      </bpmn:extensionElements>
      <bpmn:incoming>success-authentication-flow</bpmn:incoming>
      <bpmn:outgoing>created-token-flow</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="check-login-request-task" name="check username, password, and JSON body">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="check-login-request" />
        <zeebe:ioMapping />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0e4mtwd</bpmn:incoming>
      <bpmn:outgoing>Flow_1r64rs3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="gateway_is_authorized" name="was the username,password correct?">
      <bpmn:incoming>Flow_1r64rs3</bpmn:incoming>
      <bpmn:outgoing>success-authentication-flow</bpmn:outgoing>
      <bpmn:outgoing>failed-authentication-flow</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="success-authentication-flow" name="yes" sourceRef="gateway_is_authorized" targetRef="create-token-task">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=loginValid = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="created-token-flow" sourceRef="create-token-task" targetRef="send_success_user_login_revent" />
    <bpmn:sequenceFlow id="failed-authentication-flow" name="No" sourceRef="gateway_is_authorized" targetRef="event_send_error_respond">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=loginValid = false</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_1r64rs3" sourceRef="check-login-request-task" targetRef="gateway_is_authorized" />
    <bpmn:sequenceFlow id="Flow_0e4mtwd" sourceRef="login_request_event" targetRef="check-login-request-task" />
    <bpmn:endEvent id="event_send_error_respond" name="send erorr respond">
      <bpmn:extensionElements />
      <bpmn:incoming>failed-authentication-flow</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="send_success_user_login_revent" name="send successful respond">
      <bpmn:incoming>created-token-flow</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
