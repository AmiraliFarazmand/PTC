<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="PurchaseProcess" name="Purchase Process" isExecutable="true">
    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- User Authentication -->
    <bpmn:task id="Task_AuthenticateUser" name="Authenticate User">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>

    <!-- Submit Purchase Request -->
    <bpmn:task id="Task_SubmitPurchaseRequest" name="Submit Purchase Request">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>

    <!-- Create Purchase -->
    <bpmn:serviceTask id="Task_CreatePurchase" name="Create Purchase">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>

    <!-- Wait for Payment -->
    <bpmn:intermediateCatchEvent id="Event_WaitForPayment" name="Wait for Payment">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration>PT10M</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>

    <!-- Confirm Payment -->
    <bpmn:exclusiveGateway id="Gateway_PaymentReceived" name="Payment Received?">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:task id="Task_ConfirmPayment" name="Confirm Payment">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:task>

    <!-- Cancel Purchase -->
    <bpmn:task id="Task_CancelPurchase" name="Cancel Purchase">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>

    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:incoming>Flow_9</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_AuthenticateUser" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_AuthenticateUser" targetRef="Task_SubmitPurchaseRequest" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_SubmitPurchaseRequest" targetRef="Task_CreatePurchase" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_CreatePurchase" targetRef="Event_WaitForPayment" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Event_WaitForPayment" targetRef="Gateway_PaymentReceived" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Gateway_PaymentReceived" targetRef="Task_ConfirmPayment" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Gateway_PaymentReceived" targetRef="Task_CancelPurchase" />
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Task_ConfirmPayment" targetRef="EndEvent_1" />
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Task_CancelPurchase" targetRef="EndEvent_1" />
  </bpmn:process>

  <!-- Diagram -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="PurchaseProcess">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="100" y="100" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_AuthenticateUser_di" bpmnElement="Task_AuthenticateUser">
        <dc:Bounds x="200" y="90" width="100" height="56" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_SubmitPurchaseRequest_di" bpmnElement="Task_SubmitPurchaseRequest">
        <dc:Bounds x="350" y="90" width="150" height="56" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CreatePurchase_di" bpmnElement="Task_CreatePurchase">
        <dc:Bounds x="550" y="90" width="120" height="56" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_WaitForPayment_di" bpmnElement="Event_WaitForPayment">
        <dc:Bounds x="720" y="100" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_PaymentReceived_di" bpmnElement="Gateway_PaymentReceived">
        <dc:Bounds x="820" y="95" width="50" height="50" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ConfirmPayment_di" bpmnElement="Task_ConfirmPayment">
        <dc:Bounds x="920" y="50" width="120" height="56" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CancelPurchase_di" bpmnElement="Task_CancelPurchase">
        <dc:Bounds x="920" y="150" width="120" height="56" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="1100" y="100" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="136" y="118" />
        <di:waypoint x="200" y="118" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="300" y="118" />
        <di:waypoint x="350" y="118" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="500" y="118" />
        <di:waypoint x="550" y="118" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="670" y="118" />
        <di:waypoint x="720" y="118" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="756" y="118" />
        <di:waypoint x="820" y="118" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_6_di" bpmnElement="Flow_6">
        <di:waypoint x="870" y="95" />
        <di:waypoint x="920" y="78" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_7_di" bpmnElement="Flow_7">
        <di:waypoint x="870" y="145" />
        <di:waypoint x="920" y="178" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_8_di" bpmnElement="Flow_8">
        <di:waypoint x="1040" y="78" />
        <di:waypoint x="1100" y="118" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_9_di" bpmnElement="Flow_9">
        <di:waypoint x="1040" y="178" />
        <di:waypoint x="1100" y="118" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>