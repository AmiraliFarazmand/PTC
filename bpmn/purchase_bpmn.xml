<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.33.1">
  <bpmn:process id="PurchaseProcess" name="Purchase Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start Purchase Process">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_AuthenticateUser" name="Authenticate User">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_SubmitPurchaseRequest" name="Submit Purchase Request (amount, address)">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_CreatePurchase" name="Create Purchase with Pending Status">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:parallelGateway id="Gateway_PaymentOptions" name="Payment Options">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_ToPayment</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToTimer</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:intermediateCatchEvent id="Event_PaymentTimer" name="Wait 10 Minutes">
      <bpmn:incoming>Flow_ToTimer</bpmn:incoming>
      <bpmn:outgoing>Flow_TimerExpired</bpmn:outgoing>
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration>PT10M</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:userTask id="Task_MakePayment" name="User Makes Payment (purchase_id)">
      <bpmn:incoming>Flow_ToPayment</bpmn:incoming>
      <bpmn:outgoing>Flow_UserPaid</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_ConfirmPayment" name="Confirm Payment and Update Status">
      <bpmn:incoming>Flow_UserPaid</bpmn:incoming>
      <bpmn:outgoing>Flow_PaymentConfirmed</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_CancelPurchase" name="Cancel Unpaid Purchase">
      <bpmn:incoming>Flow_TimerExpired</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCancelCheck</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_CheckIfAlreadyPaid" name="Already Paid?">
      <bpmn:incoming>Flow_ToCancelCheck</bpmn:incoming>
      <bpmn:outgoing>Flow_NotPaid</bpmn:outgoing>
      <bpmn:outgoing>Flow_ConfirmPaymentCheck</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_ProcessOrder" name="Process Order (Packaging and Delivering)">
      <bpmn:incoming>Flow_1tbolq2</bpmn:incoming>
      <bpmn:outgoing>Flow_OrderProcessed</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_MergeFlows">
      <bpmn:incoming>Flow_OrderProcessed</bpmn:incoming>
      <bpmn:incoming>Flow_NotPaid</bpmn:incoming>
      <bpmn:outgoing>Flow_ToEnd</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_1" name="End Purchase Process">
      <bpmn:incoming>Flow_ToEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_AuthenticateUser" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_AuthenticateUser" targetRef="Task_SubmitPurchaseRequest" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_SubmitPurchaseRequest" targetRef="Task_CreatePurchase" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_CreatePurchase" targetRef="Gateway_PaymentOptions" />
    <bpmn:sequenceFlow id="Flow_ToPayment" sourceRef="Gateway_PaymentOptions" targetRef="Task_MakePayment" />
    <bpmn:sequenceFlow id="Flow_ToTimer" sourceRef="Gateway_PaymentOptions" targetRef="Event_PaymentTimer" />
    <bpmn:sequenceFlow id="Flow_UserPaid" sourceRef="Task_MakePayment" targetRef="Task_ConfirmPayment" />
    <bpmn:sequenceFlow id="Flow_PaymentConfirmed" sourceRef="Task_ConfirmPayment" targetRef="Gateway_MergePaymentPaths" />
    <bpmn:sequenceFlow id="Flow_TimerExpired" sourceRef="Event_PaymentTimer" targetRef="Task_CancelPurchase" />
    <bpmn:sequenceFlow id="Flow_ToCancelCheck" sourceRef="Task_CancelPurchase" targetRef="Gateway_CheckIfAlreadyPaid" />
    <bpmn:sequenceFlow id="Flow_NotPaid" name="No" sourceRef="Gateway_CheckIfAlreadyPaid" targetRef="Gateway_MergeFlows">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">#{status == "pending"}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_OrderProcessed" sourceRef="Task_ProcessOrder" targetRef="Gateway_MergeFlows" />
    <bpmn:sequenceFlow id="Flow_ToEnd" sourceRef="Gateway_MergeFlows" targetRef="EndEvent_1" />
    <bpmn:sequenceFlow id="Flow_ConfirmPaymentCheck" name="Yes" sourceRef="Gateway_CheckIfAlreadyPaid" targetRef="Gateway_MergePaymentPaths" />
    <bpmn:parallelGateway id="Gateway_MergePaymentPaths">
      <bpmn:incoming>Flow_PaymentConfirmed</bpmn:incoming>
      <bpmn:incoming>Flow_ConfirmPaymentCheck</bpmn:incoming>
      <bpmn:outgoing>Flow_1tbolq2</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:sequenceFlow id="Flow_1tbolq2" sourceRef="Gateway_MergePaymentPaths" targetRef="Task_ProcessOrder" />
  </bpmn:process>
